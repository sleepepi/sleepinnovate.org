.card.card-dimmed
  .card-body
    .text-center{ style: "font-size: 6rem;" }
      = icon("fas", "check", class: "text-warning")
    %h4.card-title Brain Quizzes
    .progress
      .progress-bar.progress-bar-striped.bg-warning{ role: "progressbar", style: "width: 100%;", aria: { valuenow: "100", valuemin: "0", valuemax: "100" } }
    %p.card-text
      .card-information Complete!
